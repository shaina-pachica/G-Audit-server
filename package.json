{
  "name": "g-server",
  "version": "1.0.0",
  "description": "",
  "main": "src/index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "generate:migration": "npx sequelize-cli migration:generate --name",
    "generate:seed": "npx sequelize-cli seed:generate --name",
    "migrate": "npx @dotenvx/dotenvx run -f ./.env -- sequelize-cli db:migrate",
    "migrate:undo": "npx @dotenvx/dotenvx run -f ./.env -- sequelize-cli db:migrate:undo:all",
    "seed": "npx @dotenvx/dotenvx run -f ./.env -- sequelize-cli db:seed:all",
    "seed:undo": "npx @dotenvx/dotenvx run -f ./.env -- sequelize-cli db:seed:undo:all",
    "start": "npm run migrate && node dist",
    "build": "rimraf dist && tsc",
    "dev": "nodemon -e ts --exec 'npm run build && npm start'",
    "dev:w": "nodemon -e ts --exec \"npm run build && npm start\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/shaina-pachica/G-Audit-server.git"
  },
